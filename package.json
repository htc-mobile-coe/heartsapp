{
  "name": "ufhnreactnative",
  "version": "3.11.0",
  "private": true,
  "xcodeSchemes": {
    "Debug": [
      "heartsapp"
    ],
    "Release": [
      "heartsapp"
    ],
    "projectDirectory": "ios",
    "settings": {}
  },
  "scripts": {
    "android": "react-native run-android --variant=devDebug",
    "androidQa": "react-native run-android --variant=qaDebug",
    "androidProd": "react-native run-android --variant=prodDebug",
    "iphoneQA": "react-native run-ios --simulator=\"iPhone 8\" --scheme \"heartsapp-QA\" --configuration QADebug",
    "iphone": "react-native run-ios --simulator=\"iPhone 8\" --scheme \"heartsapp-DEV\" --configuration DevDebug",
    "iphoneProd": "react-native run-ios --simulator=\"iPhone 8\" --scheme \"heartsapp\" --configuration Debug",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --max-warnings=0 .",
    "storybook": "start-storybook -p 8085",
    "validate": "concurrently \"npm run lint\" \"npm run test\"",
    "postversion": "react-native-version",
    "postinstall": "concurrently \"react-native-schemes-manager all\" \"patch-package\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
